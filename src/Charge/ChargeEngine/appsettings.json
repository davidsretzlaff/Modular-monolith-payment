{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ChargeEngine": {
    "MaxConcurrency": 10,
    "BatchSize": 50,
    "PollingInterval": "00:01:00",
    "MaxRetries": 3,
    "RetryDelay": "00:00:30",
    "Timeout": "00:00:30",
    "EnableCircuitBreaker": true,
    "CircuitBreakerThreshold": 5,
    "CircuitBreakerDuration": "00:01:00",
    "EnableBatching": true,
    "EnableParallelProcessing": true
  }
} 